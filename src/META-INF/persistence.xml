<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="TED" transaction-type="RESOURCE_LOCAL">
		<class>model.User</class>
		<class>model.PersonalData</class>
		<class>model.PdStatus</class>
		<class>model.Friend</class>
		<class>model.Arthro</class>
		<class>model.SxolioArthro</class>
		<class>model.Interest</class>
		<class>model.Aggelia</class>
		<class>model.Aitisi</class>
		<class>model.AdminUser</class>
		<class>model.Active</class>
		<class>model.Chat</class>
		<class>model.ChatData</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/ted"/>
			<property name="javax.persistence.jdbc.user" value="root"/>
			<property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
		</properties>
	</persistence-unit>
</persistence>
